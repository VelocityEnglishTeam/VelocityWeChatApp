<!-- src/pages/ScoreAnalysis/result.wxml -->
<view class="caption-wrap">
    <view wx:for="{{comments}}" wx:key="id">
        <view wx:if="{{item.comment}}">
            <i-collapse-item title="{{item.name_cn}}">
                <view slot="content">
                    <text>{{item.comment}}</text>
                </view>
            </i-collapse-item>
        </view>
    </view>
</view>